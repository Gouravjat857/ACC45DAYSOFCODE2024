#include <bits/stdc++.h>
using namespace std;

int main() {
    int T; // Number of test cases
    cin >> T;
    
    while (T--) {
        int X, Y;
        cin >> X >> Y;
        
        // Calculate maximum months of renting such that cost < purchase cost
        int maxMonths = (Y - 1) / X;
        
        // Output the result for the current test case
        cout << maxMonths << endl;
    }
    
    return 0;
}
